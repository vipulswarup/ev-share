<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="webscripts.resources.department" class="org.alfresco.i18n.ResourceBundleBootstrapComponent">
		<property name="resourceBundles">
			<list>
				<value>alfresco.web-extension.messages.department</value>
				<value>alfresco.web-extension.messages.custom</value>
			</list>
		</property>
	</bean>

	<!-- Add module specific messages and labels - Audit Trail changes  added by Raghav Bhardwaj on 3 -11-15-->
	<bean id="com.eisenvault.share-amp.resources"
		class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent">
		<property name="resourceBundles">
			<list>
				<value>alfresco.web-extension.messages.callws</value>
			</list>
		</property>
	</bean>
	

</beans>
